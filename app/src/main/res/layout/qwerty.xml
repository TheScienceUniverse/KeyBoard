		android:gravity="center"
		android:textSize="@dimen/key_val_primary_size"
		android:textColor="@android:color/black"
		android:text="Alt"
	/>
	<TextView
		android:id="@+id/key_right_function"
		android:layout_width="@dimen/key_box_width"
		android:layout_height="@dimen/key_box_height"
		android:layout_below="@id/key_left_shift"
		android:layout_toEndOf="@+id/key_right_alt"
		android:layout_toRightOf="@+id/key_right_alt"
		android:background="@drawable/key_background"
		android:gravity="center"
		android:textSize="@dimen/key_val_primary_size"
		android:textColor="@android:color/black"
		android:text="Fn"
	/>
	<ImageView
		android:id="@+id/key_menu"
		android:layout_width="@dimen/key_box_width"
		android:layout_height="@dimen/key_box_height"
		android:layout_below="@id/key_left_shift"
		android:layout_toEndOf="@+id/key_right_function"
		android:layout_toRightOf="@+id/key_right_function"
		android:background="@drawable/key_background"
		android:src="@drawable/key_menu"
	/>
	<ImageView
		android:id="@+id/key_right_control"
		android:layout_width="@dimen/key_box_width_1_5"
		android:layout_height="@dimen/key_box_height"
		android:layout_below="@id/key_left_shift"
		android:layout_toEndOf="@+id/key_menu"
		android:layout_toRightOf="@+id/key_menu"
		android:background="@drawable/key_background"
		android:gravity="center"
		android:textSize="@dimen/key_val_primary_size"
		android:textColor="@android:color/black"
		android:src="@drawable/key_control"
	/>
</RelativeLayout>